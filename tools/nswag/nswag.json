{
  "runtime": "Net60",
  "documentGenerator": {
    "fromDocument": {
      "url": "../../src/app/reference/swagger.json"
    }
  },
  "codeGenerators": {
    "openApiToTypeScriptClient": {
      "className": "NSwagService",
      "template": "Angular",
      "withCredentials": false,
      "useSingletonProvider": true,
      "injectionTokenType": "InjectionToken",
      "rxJsVersion": 7.8,
      "dateTimeType": "string",
      "nullValue": "Undefined",
      "generateClientClasses": true,
      "generateClientInterfaces": false,
      "exportTypes": true,
      "generateDtoTypes": true,
      "operationGenerationMode": "SingleClientFromPathSegments",
      "markOptionalProperties": false,
      "typeStyle": "Interface",
      "enumStyle": "StringLiteral",
      "clientBaseClass": "BaseService",
      "useGetBaseUrlMethod": true,
      "output": "../../src/app/services/generated-services.ts"
    }
  }
}
